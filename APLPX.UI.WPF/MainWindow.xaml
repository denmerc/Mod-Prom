                                </Button>
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>
                        
                    </DockPanel>
                </Grid>
            </controls:Flyout>
            <controls:Flyout x:Name="Folders" Header="Add Folders:"
                             Position="Right" IsOpenChanged="MessageCenter_IsOpenChanged" 
                             Theme="Accent" Unloaded="Flyout_Unloaded" ContextMenuClosing="MessageCenter_ContextMenuClosing" >
                <Grid Margin="10"
                      Width="400">
                    <DockPanel>
                        <TextBlock DockPanel.Dock="Top" Text=""
                               Foreground="{DynamicResource IdealForegroundColorBrush}" />
                        <GroupBox  Margin="10" VerticalAlignment="Stretch" Header="Pricing">
                            <StackPanel >

                                <TextBlock Margin="10 0 0 0" x:Name="FolderTitle" FontSize="30" Foreground="White" Text="Select Folders" />
                                <TextBlock Margin="10 0 0 0" FontSize="18" Foreground="White" Text="All OPGI Parts" />
                                <TextBlock  Margin="10 0 0 0" FontSize="18" Foreground="White" Text="Status: Active"  />


                                <ListView Margin="10,10,10,23" MinHeight="250" BorderThickness="0" Width="Auto" Background="{StaticResource HighlightBrush}" FontSize="16" Foreground="White" FontWeight="Bold" Padding="0,0,0,6">
                                    <!--<ListViewItem Content="1) My Analytics" Foreground="{StaticResource MenuItemDisabledBrush}"/>
                                <ListViewItem Content="2) Identity" Foreground="White" FontSize="16" BorderThickness="0"/>-->
                                    <ListViewItem Background="Transparent" Foreground="White">

                                        <StackPanel >
                                            <TextBlock Text="Selected Analytic Folders:"></TextBlock>
                                            <Separator></Separator>
                                            <ListBox Background="{StaticResource HighlightBrush}" Foreground="White"  SelectionMode="Multiple">
                                                <ListBoxItem FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Folder 1</ListBoxItem>
                                                <ListBoxItem FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Folder 2</ListBoxItem>
                                                <ListBoxItem  FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Folder 3</ListBoxItem>
                                                <ListBoxItem FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Folder 4</ListBoxItem>
                                                <ListBoxItem FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Folder 5</ListBoxItem>
                                            </ListBox>
                                        </StackPanel>

                                    </ListViewItem>
                                    <ListViewItem Background="Transparent" Foreground="White">

                                        <StackPanel >
                                            <TextBlock  Text="Included Pricing Folders:"></TextBlock>
                                            <Separator></Separator>
                                            <ListBox Background="{StaticResource HighlightBrush}" Foreground="White">
                                                <ListBoxItem  FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Folder 1</ListBoxItem>
                                                <ListBoxItem FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Folder 2</ListBoxItem>
                                                <ListBoxItem  FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Folder 3</ListBoxItem>
                                                <ListBoxItem  FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Folder 4</ListBoxItem>
                                                <ListBoxItem  FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Folder 5</ListBoxItem>
                                            </ListBox>
                                        </StackPanel>

                                    </ListViewItem>

                                    <ListViewItem Foreground="{StaticResource MenuItemDisabledBrush}"/>

                                    <StackPanel >
                                        <TextBlock Text="Included Value Drivers:"></TextBlock>
                                        <Separator></Separator>
                                        <ListBox Background="{StaticResource HighlightBrush}" Foreground="White">
                                            <ListBoxItem  FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Value Driver 1</ListBoxItem>
                                            <ListBoxItem FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Value Driver 2</ListBoxItem>
                                            <ListBoxItem  FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Value Driver 3</ListBoxItem>
                                            <ListBoxItem  FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Value Driver 4</ListBoxItem>
