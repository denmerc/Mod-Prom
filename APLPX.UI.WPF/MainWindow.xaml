                                        <ListBox Background="{StaticResource HighlightBrush}" Foreground="White">
                                                <ListBoxItem  FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Price List 1</ListBoxItem>
                                                <ListBoxItem FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Price List 2</ListBoxItem>
                                                <ListBoxItem  FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Price List 3</ListBoxItem>
                                                <ListBoxItem  FontWeight="ExtraLight" Background="{StaticResource HighlightBrush}">Price List 4</ListBoxItem>
